<h1>Countries#index</h1>
<p>Find me in app/views/countries/index.html.erb</p>

<%= link_to("add new country", new_country_path) %>

<table>
    <tr>
        <th>ID</th>
        <th>name</th>
        <th>code</th>
        <th>links</th>
    </tr>
        <% @countries.each do |country| %>
            <tr>
            <td><%= country.id %></td>
            <td><%= country.country_name %></td>
            <td><%= country.country_code %></td>
            <td>
            <%= link_to("view", country_path(country.id)) %><br>
            <%= link_to("edit", edit_country_path(country.id)) %><br>
            <%= link_to("delete", delete_country_path(country.id)) %><br>
            </tr>
        <% end %>
    
</table>